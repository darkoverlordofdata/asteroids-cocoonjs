  "private": true,
  "ignore": [
    "**/.*",
    "tmp",
    "build",
    "node_modules",
    "packages",
    "web/packages",
    "test",
    "tests"
  ],
  "dependencies": {
    "cocoonjs": "git://github.com/ludei/cocoonjs-plugins#~3.0.4",
    "ash": "git://github.com/darkoverlordofdata/ash.coffee",
    "box2d": "git://github.com/darkoverlordofdata/Box2dWeb",
    "touch-control": "git://github.com/eugenioclrc/phaser-touch-control-plugin",
    "game-controller": "git://github.com/darkoverlordofdata/html5-virtual-game-controller",
    "stats.js": "*",
    "pixi.js": "~2.2.8",
    "phaser": "2.3.0",
    "localstoragedb": "~2.3.1"
  },
  "packages": {
    "cocoonjs": "cocoonjs/build/*.*",
    "ash": "ash/dist/*.*",
    "box2d": "box2d/dist/*.js",
    "touch-control": "touch-control/src/*.js",
    "game-controller": "game-controller/*.js",
    "stats.js": "stats.js/build/*.js",
    "pixi.js": "pixi.js/bin/*.*",
    "phaser": "phaser/build/custom/*.*",
    "localstoragedb": "localstoragedb/*.js"
  }
